// <![CDATA[
/*global jQuery, document */

function restartFooter(settings, html) {
    "use strict";
    jQuery(settings.element).append(html);
    jQuery("#myCarousel-footer-5star").carousel({
        interval: 10000 // 10 secs
    });
}

